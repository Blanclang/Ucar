/**app.wxss**/
view,span,p,navigator{
  box-sizing: border-box;
  font-size: 30rpx;
}
image {
  display:block;
}

.red{
  border: 1px solid red;
}
.blue{
  border: 1px solid blue;
}
/* flex布局 */
.flex_cc{
  display: flex;
  justify-content: center;
  align-items: center;
}
.flex_lc{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.flex_rc{
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.flex_sc{
  display: flex;
  justify-content: space-between;
  align-items: center;
}

